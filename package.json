  "bugs": {
    "url": "https://github.com/MarleeG/RPS/issues"
  },
  "homepage": "https://github.com/MarleeG/RPS#readme",
  "dependencies": {
    "animate.css": "^4.1.0",
    "concurrently": "^5.2.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
